{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["google", "plugin:prettier/recommended", "prettier"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint", "prettier"],
	"rules": {
		"@typescript-eslint/explicit-function-return-type": ["warn"],
		"max-len": ["warn", {"code": 120}]
	}
}
